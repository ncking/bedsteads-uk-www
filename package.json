  "keywords": [
    "react",
    "preact",
    "jamstack",
    "node",
    "typescript",
    "ssr",
    "cypress",
    "docker",
    "vite"
  ],
  "sideEffects": false,
  "scripts": {
    "start:deno": "deno --allow-run --allow-env --allow-read --allow-sys --allow-ffi --allow-net ./.server/init.js",
    "start": "node ./.server/init.js",
    "dev": "nuggins dev",
    "build": "nuggins build && npm run build:sw",
    "build:docker": "nuggins build --docker",
    "build:server": "nuggins build --server",
    "build:client": "nuggins build --client",
    "build:sw": "tsx ./service-worker/build-sw.js",
    "build:favicons": "tsx _scripts/favicons/build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write \"{src,test}/**/*.{js,ts,tsx}\"",
    "lint": "eslint ./src",
    "lint:sass": "stylelint --allow-empty-input \"**/*.{css,scss}\"",
    "lint:sass:fix": "stylelint --allow-empty-input \"**/*.{css,scss}\" --fix",
    "lint:fix": "eslint ./src  --fix"
  },
  "overrides": {
    "@testing-library/cypress": {
